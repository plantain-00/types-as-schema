{
  "name": "types-as-schema",
  "version": "1.1.0",
  "description": "Genetate json scheme or protobuf file from types.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "tsc": "tsc -p src/",
    "tslint": "tslint \"src/**/*.ts\"",
    "demoTest": "./bin/types-as-schema demo/types.ts --json demo/ --debug demo/debug.json --protobuf demo/types.proto",
    "demoMatch": "./bin/types-as-schema demo/match.ts --json demo/ --debug demo/match-debug.json",
    "demoCases": "./bin/types-as-schema demo/cases.ts --json demo/ --debug demo/cases-debug.json --protobuf demo/cases.proto",
    "test": "tsc -p spec && jasmine",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "release": "clean-release",
    "build": "npm run clean && npm run tsc",
    "lint": "npm run tslint && npm run standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/types-as-schema.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/types-as-schema/issues"
  },
  "homepage": "https://github.com/plantain-00/types-as-schema#readme",
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/minimist": "1.2.0",
    "@types/node": "8.0.14",
    "clean-release": "1.3.2",
    "jasmine": "2.6.0",
    "rimraf": "2.6.1",
    "standard": "10.0.2",
    "tslint": "5.5.0"
  },
  "dependencies": {
    "minimist": "1",
    "typescript": "2"
  },
  "peerDependencies": {
    "typescript": "2"
  },
  "bin": {
    "types-as-schema": "bin/types-as-schema"
  }
}
