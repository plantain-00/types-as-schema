{
  "info": {
    "title": "Sample API",
    "description": "API description in Markdown.",
    "version": "1.0.0"
  },
  "host": "api.example.com",
  "basePath": "/v1",
  "schemes": [
    "https"
  ],
  "swagger": "2.0",
  "paths": {
    "/pet/{id}": {
      "get": {
        "operationId": "getPetById",
        "parameters": [
          {
            "name": "status",
            "required": true,
            "in": "query",
            "schema": {
              "type": "string",
              "enum": [
                "health",
                "sick"
              ]
            }
          },
          {
            "name": "tags",
            "required": true,
            "in": "body",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "minItems": 1
            }
          },
          {
            "name": "pet",
            "required": true,
            "in": "body",
            "schema": {
              "$ref": "#/definitions/Pet"
            }
          },
          {
            "name": "id",
            "required": false,
            "in": "path",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "default": 2,
              "description": "pet id"
            }
          },
          {
            "name": "sortType",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string",
              "enum": [
                "asc",
                "desc"
              ],
              "default": "asc"
            }
          }
        ],
        "summary": "get pet by id.",
        "description": "get pet by id",
        "deprecated": true,
        "tags": [
          "pet"
        ],
        "responses": {
          "200": {
            "schema": {
              "$ref": "#/definitions/Pet"
            }
          }
        }
      }
    },
    "/download": {
      "get": {
        "operationId": "downloadFile",
        "parameters": [],
        "responses": {
          "200": {
            "schema": {
              "type": "file"
            }
          }
        }
      }
    },
    "/empty": {
      "get": {
        "operationId": "returnEmpty",
        "parameters": [],
        "responses": {
          "200": {
            "schema": {}
          }
        }
      }
    },
    "/upload": {
      "post": {
        "consumes": [
          "multipart/form-data"
        ],
        "operationId": "uploadFile",
        "parameters": [
          {
            "name": "file",
            "required": true,
            "in": "formData",
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "schema": {}
          }
        }
      }
    }
  },
  "definitions": {
    "Pet": {
      "type": "object",
      "properties": {
        "id": {
          "type": "number"
        },
        "name": {
          "type": "string"
        },
        "photoUrls": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "status": {
          "type": "string",
          "enum": [
            "available",
            "pending",
            "sold"
          ]
        }
      },
      "required": [
        "name",
        "photoUrls",
        "status"
      ],
      "additionalProperties": false
    }
  }
}